{
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test-build": "zsh -c 'esbuild ${TEST_DIR:-challenge}/**/*.jsx --jsx=automatic --log-level=error --outdir=_test'",
