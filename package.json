    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "esbuild": "^0.15.10",
    "eslint": "^8.25.0",
    "eslint-config-react-minimal": "^3.0.0",
    "jsdom": "^20.0.1",
    "react-test-renderer": "^18.2.0",
    "vite": "^3.1.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "eslint-config-react-minimal"
    ],
    "env": {
      "es2021": true
    }
  },
  "volta": {
    "node": "18.10.0"
  }
